                    if (row.sincronizado == 1) {
                        if (row.produto_ativo == 0) {
                            Sexy.alert('Este produto Não pode ser apagado pois foi sincronizado com o PDV e já está desativado.');
                            return false;
                        } else {
                            $$('input[name=delete]').set('confirm', 'Este produto Não pode ser removido, pois foi sincronizado com o PDV. Deseja desativar este produto?');

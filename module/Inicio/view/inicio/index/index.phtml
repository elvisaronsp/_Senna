(function($){
	
	$(document).ready(function(){
		
		//autosuggest de funcionario
		funcionario_autosuggest();
		
		//Monta as Abas
		monta_abas();
		
	});
	
	/**
	 * Montando abas especiais para dashboard
	 */
	function monta_abas(){
		$(".tab_selector li a").click(function(){
			var $this = $(this);
			var ativa = $this.parents("ul:first").find(".tab_sel_ativa");
			if(!$this.parent().is(ativa)){
				ativa.removeClass("tab_sel_ativa");
				$this.parent().addClass("tab_sel_ativa");
			}
			$("#dashboard_type").val($(this).attr("ref"));
			$('#form').submit();
		});
	}
	

	/**
	 * Tratamento de autosuggest de funcionario
	 */
	function funcionario_autosuggest(){
		$("#id_funcionario").bind("change", function(evt, el, item){
			$("#nome_funcionario").val(item.value);
			
			//Se for funcionario
			if(item.depto == undefined)
				$('#func_depto').val('0');
			//Se For departamento
			else	
				$('#func_depto').val('1');
		});
	}
	
})(jQuery);

            var get_cep_ws = function (container) {
                //Carregando
                $("#loader").show().find(".carregando").hide();
                $("#loader").find(".enviando").show();

                var cep = $(container).find("[name*=endereco__cep]").val();
                var url = '{url_ws_cep}';


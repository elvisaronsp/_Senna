	<script type='text/javascript'>


/********************************
../views/common/home/redirect.js
********************************/

jQuery.noConflict();

(function($){
	$(window).load(function(){
	
		//recupera o hash
		var hash = location.hash;
		
		//caso o hash não tenha sido definido ou seja vazio, não continua
		if(hash == undefined || hash == "") return;
		
		//impede que o reload execute a ação novamente
		location.hash = "";
		
		//clica no link para redirect
		var uri = hash.replace("#", "");
		var a = $("a[id="+uri+"]").last();
		if(a.length){
			//hask pra funcionar no chrome
			setTimeout(function(){ a.get(0).click(); }, 100);
		}
		//link não encontrado. Abre em uma nova janela (apondando pra dentro do sistema)
		else{
			parent.MochaUI.openWindow({
				id: "senna//"+uri,
				title: uri,
				contentURL: "senna//"+uri
			});
		}
		
	});
})(jQuery);
</script>

